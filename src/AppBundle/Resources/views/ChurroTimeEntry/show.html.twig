{% extends 'AppBundle::base.html.twig' %}

{% block body %}
    <div class="container">
        <h1>Time entry {{ timeEntry.id }}</h1>
        <table class="table">
            <tbody>
                <tr>
                    <th>ID</th>
                    <td>{{ timeEntry.id }}</td>
                </tr>
                <tr>
                    <th>Cook time</th>
                    <td>
                        {{ timeEntry.startCookingAt|date('H:i') }}
                        -
                        {{ timeEntry.endCookingAt|date('H:i') }}
                    </td>
                </tr>
                <tr>
                    <th>Cleanup time</th>
                    <td>
                        {{ timeEntry.startCleanupAt|date('H:i') }}
                        -
                        {{ timeEntry.endCleanupAt|date('H:i') }}
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
{% endblock %}

